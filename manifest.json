{
  "manifest_version": 2,

  "name": "Tracking monitor",
  "description": "Bla bla bla",
  "version": "0.1.1",
  "background": {
    "scripts": ["jquery.js", "background.js", "jquery-3.2.1.min.js"]
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon_128.png"
  },
  "permissions": [
   "activeTab",
   "idle",
   "tabs",
   "storage",
   "webNavigation"
   ]
}