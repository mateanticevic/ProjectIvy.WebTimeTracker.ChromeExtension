{
  "manifest_version": 2,

  "name": "Tracking monitor",
  "description": "Bla bla bla",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
   "idle",
   "tabs",
   "storage",
   "webNavigation"
   ]
}